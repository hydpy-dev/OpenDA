<?xml version="1.0" encoding="UTF-8"?>
<hpcmr:config xmlns="https://github.com/hydpy-dev/hydpy/releases/download/your-hydpy-version/HydPyConfigBase.xsd" xmlns:hpcmr="https://github.com/hydpy-dev/hydpy/releases/download/your-hydpy-version/HydPyConfigMultipleRuns.xsd">
  <options/>
  <timegrid>
    <firstdate>1996-01-01T00:00:00</firstdate>
    <lastdate>1996-02-10T00:00:00</lastdate>
    <stepsize>1d</stepsize>
  </timegrid>
  <add_selections>
    <add_selection>
      <name>gauge</name>
      <devices>lahn_1</devices>
    </add_selection>
    <add_selection>
      <name>catchment</name>
      <devices>land_lahn_1</devices>
    </add_selection>
  </add_selections>
  <selections>
    gauge
    catchment
  </selections>
  <conditions_io>
    <inputdir>init_1996_01_01_00_00_00</inputdir>
    <outputdir>irrelevant</outputdir>
    <zip>true</zip>
  </conditions_io>
  <series_io>
    <readers>
      <reader info="all input data">
        <sequences>
          <hland_v1>
            <inputs>
              <p/>
              <t/>
              <tn/>
              <epn/>
            </inputs>
          </hland_v1>
        </sequences>
      </reader>
      <reader>
        <directory>../../../openda_projects/PF/data</directory>
        <sequences>
          <node>
            <obs/>
          </node>
        </sequences>
      </reader>
    </readers>
    <writers>
      <writer>
        <sequences>
          <node>
            <sim/>
          </node>
          <hland_v1>
            <states>
              <lz/>
            </states>
          </hland_v1>
        </sequences>
        <filetype>nc</filetype>
        <mode>jit</mode>
      </writer>
    </writers>
  </series_io>
  <exchange>
    <setitems>
      <hland_v1>
        <states>
          <lz>
            <name>lz</name>
            <level>device</level>
          </lz>
        </states>
      </hland_v1>
    </setitems>
    <getitems>
      <nodes>
        <sim.series/>
        <obs.series/>
      </nodes>
    </getitems>
  </exchange>
</hpcmr:config>